{
  "name": "lioness-utils",
  "version": "0.0.1",
  "description": "A set of utils to parse react files and export .po files and import them back",
  "bin": "src/cli.js",
  "main": "src/main.js",
  "engines": {
    "node": "~10.14.0",
    "npm": "~6.4.0"
  },
  "scripts": {
    "build": "tsc",
    "build:clean": "npm run clean && npm run build",
    "watch": "tsc -w",
    "lint": "npm run lint:tsc && npm run lint:tslint",
    "lint:tsc": "tsc -p tsconfig.json --noEmit",
    "lint:tslint": "tslint -c tslint.json -p tsconfig.json --fix",
    "lint:prettier": "prettier --write \"{src,test}/**/*.ts*\"",
    "start": "node src/main",
    "start:dev": "concurrently --kill-others \"tsc -w\" \"set DEBUG=* && nodemon src/main.js\"",
    "commit": "commit-prompt --no-verify",
    "precommit": "lint-staged && npm run lint",
    "test": "jest --detectOpenHandles",
    "test:unit": "jest --detectOpenHandles --testPathPattern \"src/.+spec.tsx?$\"",
    "test:e2e": "jest --detectOpenHandles --testPathPattern \"__tests__/.+.tsx?\"",
    "test:ci": "jest --detectOpenHandles --coverage",
    "clean": "rimraf '{src,__tests__}/**/*.js?(x)?(.map)'",
    "release": "./extra/deployment/release.sh",
    "prepare": "npm run build:clean"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "keywords": [
    "lioness",
    "gettext",
    "react",
    "i18n",
    "intl"
  ],
  "author": "goooseman",
  "license": "MIT",
  "bugs": {
    "url": ""
  },
  "homepage": "",
  "devDependencies": {
    "@material-ui/core": "^3.9.2",
    "@types/fs-extra": "^5.0.5",
    "@types/jest": "^23.3.13",
    "@types/yargs": "^12.0.9",
    "concurrently": "^4.1.0",
    "jest": "^23.6.0",
    "lioness": "^0.2.13",
    "nodemon": "^1.18.10",
    "prettier-config-trucknet-io": "^2.0.1",
    "react": "^16.8.2",
    "react-dom": "^16.8.2",
    "rimraf": "^2.6.3",
    "trucknet-commit": "^1.4.3",
    "ts-jest": "^23.10.3",
    "tslint": "^5.12.1",
    "tslint-config-trucknet-io": "^1.4.0"
  },
  "_moduleAliases": {
    "@src": "src"
  },
  "dependencies": {
    "@types/glob": "^7.1.1",
    "@types/node": "^10.12.21",
    "fs-extra": "^7.0.1",
    "gettext-parser": "^3.1.0",
    "glob": "^7.1.3",
    "module-alias": "^2.1.0",
    "react-gettext-parser": "^1.8.2",
    "typescript": "^3.3.1",
    "yargs": "^13.2.1"
  }
}
